FUNCTION main()
{
    NUMBER I = 0;
    REPEAT(I < 100):
    {
        IF(I % 5 == 0 && I % 7 == 0): 
        {
            SPEAK("FizzBuzz");
        }
        ELSE IF (I % 5 == 0):
        {
            SPEAK("Fizz");
        }
        ELSE IF (I % 7 == 0):
        {
            SPEAK("Buzz");
        }
        ELSE:
        {
            SPEAK(I);
        }
     I += 1;    
    }
}
